<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">	
 	<mapper namespace="com.gudee.urban.dao.MapMapper">
 		<select id="getList" resultType="MapVO">
			SELECT * FROM real_estate
 			WHERE id = 1
 		</select>
 		
 		<insert id="addList" parameterType="MapVO">
 			INSERT INTO test_apt
 			VALUES(#{id}, #{apt_name}, #{transaction_price}, #{transaction_yy}, #{transaction_mm}, #{transaction_dd}, 
 					#{rode_name}, #{street_number}, #{build_yy}, #{floor}, #{area}, #{x_coordinate}, #{y_coordinate})
 		</insert>
 	</mapper>
